@model Spotify.Application.Musicians.GetMusicians.MusicianModel


<div class="card">
    <a asp-page="/Musician/Profile" asp-route-id="@Model.Id">
        <div class="card-image">
            <figure class="image is-square">
                <img src="~/images/band-pictures/placeholder.jpg" alt="@Model.Name">
            </figure>
        </div>
        <div class="card-content">
            <p class="title is-5 has-text-centered">
                @Model.Name
            </p>
        </div>
    </a>
</div>